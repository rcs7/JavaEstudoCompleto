package oo.heranca;

public class TestDrive {

	public static void main(String[] args) {
       Carro c = new Civic();
       c.acelerar();
       System.out.println(c.getVelocidadeAtual());
	
       //a variável c mudou!
       c = new Ferrari();
       c.acelerar();
       System.out.println(c.getVelocidadeAtual());
	   
       Ferrari f = new Ferrari();
       System.out.println(f.imprimiVelMaxima());
       
       System.out.println(f.imprimiVelMaximaFerrari()); 
       
	}

}
